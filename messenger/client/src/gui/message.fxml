<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.text.Font?>

<Pane xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1">
    <Label fx:id="name" style="-fx-background-color: white; -fx-background-radius: 5;">
        <font>
            <Font name="System Bold Italic" size="13.0"/>
        </font>
        <padding>
            <Insets bottom="2.0" left="5.0" right="5.0" top="2.0"/>
        </padding>
    </Label>
    <Label fx:id="message" layoutX="47.0" style="-fx-background-color: white; -fx-background-radius: 5;">
        <padding>
            <Insets bottom="3.0" left="5.0" right="5.0" top="3.0"/>
        </padding>
    </Label>
</Pane>